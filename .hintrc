{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "css-prefix-order": "off",
    "axe/parsing": "off",
    "axe/name-role-value": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "axe/structure": "off"
  }
}